{
	"name": "GridMap",
	"version": "1.0.0",
	"description": "GridMap is HeatMap by default, but you can do many more weird stuff with it.",
	"author": "akashgoswami90s@gmail.com",
	"license": "MIT",

	"dependencies": { },

	"devDependencies": {
		"jshint": "^2.9.1"
	},
    
	"jshintConfig": {
        "bitwise": false,
        "camelcase": true,
        "curly": true,
        "eqeqeq": false,
        "es3": false,
        "forin": false,
        "freeze": true,
        "immed": true,
        "indent": 4,
        "latedef": true,
        "newcap": true,
        "noarg": true,
        "noempty": true,
        "nonew": false,
        "plusplus": false,
        "quotmark": "single",
        "undef": true,
        "unused": true,
        "trailing": true,
        "asi": false,
        "boss": true,
        "debug": false,
        "eqnull": true,
        "evil": false,
        "expr": true,
        "funcscope": false,
        "globalstrict": false,
        "iterator": false,
        "lastsemic": false,
        "laxbreak": false,
        "laxcomma": false,
        "loopfunc": false,
        "maxlen": 120,
        "multistr": false,
        "notypeof": false,
        "proto": false,
        "scripturl": false,
        "smarttabs": false,
        "shadow": false,
        "sub": false,
        "supernew": false,
        "browser": false,
        "devel": false,
        "node": true,
        "onevar": true,
        "globals": {
            "GridMap": true,
            "window": true
        }
    }
}
